{
    "name": "FocusCheck",
    "description": "A popout to do list that blocks all social media until all tasks are completed",
    "version": "0.01",
    "browser_action": {
        "default_title": "FocusCheck",
        "default_popup": "popout.html"
    },
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["Content.js"]
        }
    ]
}